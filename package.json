{
  "name": "superbox",
  "version": "3.0.0-0",
  "description": "Primitive React component for all your styles",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "babel src -d dist",
    "logo": "repng docs/logo.js -d docs -f logo.png -w 256 -h 256",
    "logo-svg": "scrs docs/logo.js --svg > docs/logo.svg",
    "start": "mdx-go docs",
    "test": "jest"
  },
  "keywords": [],
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "known-css-properties": "^0.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "jest": "^23.4.1",
    "jest-styled-components": "^6.3.1",
    "mdx-go": "^2.0.0-31",
    "prettier": "^1.13.7",
    "react": "^16.6.3",
    "react-test-renderer": "^16.6.3",
    "repng": "^3.0.0",
    "scrs": "^1.1.0",
    "styled-components": "^4.1.1"
  },
  "peerDependencies": {
    "styled-components": ">=4.0.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-styled-components",
    "testMatch": [
      "**/test/**/*.js"
    ],
    "coverageReporters": [
      "html",
      "lcov"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
